<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


    <bean name="score" class="com.sys.yang.standard2.Score">
        <constructor-arg name="type" value="java"/>
        <constructor-arg name="level" value="A"/>
    </bean>

    <bean name="student" class="com.sys.yang.standard2.Student">
        <constructor-arg name="id" value="1"/>
        <constructor-arg name="name" value="lisi"/>
        <constructor-arg name="age" value="18"/>
        <constructor-arg name="score" ref="score"/>
    </bean>







</beans>